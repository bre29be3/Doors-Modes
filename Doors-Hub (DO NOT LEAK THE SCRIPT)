    Title = "Script Loading!";
    Text = "Loading 1/2!";
    Duration = 5;
})

    end

    local SkeletonKey = game.Players.LocalPlayer.PlayerGui.MainUI.ItemShop.Items["ItemShop_Skeleton Key"]

if SkeletonKey.Visible == false then
    SkeletonKey.Visible = true
    local StarterGui = game:GetService("StarterGui")

    StarterGui:SetCore("SendNotification", {
        Title = "Full Pre-Run Shop Loaded!";
        Text = "Have fun!";
        Duration = 5;
    })
else
    local StarterGui = game:GetService("StarterGui")

    StarterGui:SetCore("SendNotification", {
        Title = "Doors Full Pre-Run Shop!";
        Text = "Have Fun!";
        Duration = 5;
    })
end
    print("Clicked")
end)
